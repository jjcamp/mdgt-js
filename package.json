  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.33",
    "@types/node": "0.0.2",
    "@types/parse5": "^2.2.34",
    "@types/xmldom": "^0.1.28",
    "chai": "^3.5.0",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.4",
    "ts-node": "^1.2.2",
    "tslint": "^4.0.2",
    "typescript": "2.0.10"
  },
  "engines": {
    "node": ">=4.0.0"
  }
